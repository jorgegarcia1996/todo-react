{
  "homepage": "https://jorgegarcia1996.github.io/todo-react",
  "name": "react-todo-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "react": "^16.12.0",
    "react-confirm-alert": "^2.4.1",
    "react-dom": "^16.12.0",
    "react-scripts": "1.1.5"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "server": "json-server --watch src/mockdata/api.json --port 3001",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "sass:watch": "node-sass -w src -o src --output-style compressed --include-path src",
    "sass:build": "node-sass src -o src --output-style compressed --include-path src",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.0",
    "react-icons": "^3.8.0",
    "react-router-dom": "^5.1.2"
  }
}
